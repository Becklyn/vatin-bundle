{
    "name": "becklyn/vatin-bundle",
    "description": "Symfony bundle for the VATIN library",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "vat",
        "tax",
        "btw"
    ],
    "authors": [
        {
            "name": "David de Boer",
            "email": "david@ddeboer.nl"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ddeboer/vatin": "^2.2.2",
        "symfony/config": "^v5.4.3 || ^v6.0.3",
        "symfony/dependency-injection": "^v5.4.3 || ^v6.0.3",
        "symfony/http-kernel": "^v5.4.3 || ^v6.0.3",
        "symfony/validator": "^v5.4.3 || ^v6.0.3"
    },
    "require-dev": {
        "doctrine/annotations": "^1.13.2",
        "roave/security-advisories": "dev-latest",
        "symfony/framework-bundle": "^v5.4.3 || ^v6.0.3",
        "symfony/phpunit-bridge": "^5.4.3 || ^6.0.3",
        "symfony/yaml": "^v5.4.3 || ^v6.0.3"
    },
    "autoload": {
        "psr-4": {
            "Ddeboer\\VatinBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Ddeboer\\VatinBundle\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
